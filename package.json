{
  "name": "chirp-data",
  "version": "1.0.0",
  "description": "CHIRP Radio Content Management System",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
    "generate:types": "payload generate:types",
    "seed": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx src/seed/import-data.ts",
    "seed:mobile": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/seed-mobile-content.ts",
    "seed:onboarding": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/seed-onboarding.ts",
    "create:admin": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/create-admin.ts",
    "create:test-listener": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/create-test-listener.ts",
    "import:shop-images": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/import-shop-images.ts",
    "import:shop-images-from-urls": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/import-shop-images-from-urls.ts",
    "import:members": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/import-members.ts",
    "fix:shop-images": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/fix-shop-image-references.ts",
    "fix:shop-items": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/fix-shop-items-images.ts",
    "cleanup:media": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/cleanup-duplicate-media.ts",
    "export": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/export-data.ts",
    "export:shop-items": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/export-shop-items.ts",
    "restore": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/restore-data.ts",
    "restore:media": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/restore-media-from-backup.ts",
    "update:member-titles": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/update-member-display-titles.ts",
    "update:show-schedules": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/update-show-schedule-display.ts",
    "migrate:schedule-times": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/migrate-schedule-times.ts",
    "migrate:sqlite-to-postgres": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/migrate-sqlite-to-postgres.ts",
    "migrate:data-to-postgres": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts DATABASE_URI=postgresql://chirp:chirp_dev_password@localhost:5432/chirp_cms tsx scripts/migrate-data-to-postgres.ts",
    "migrate:improved": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts DATABASE_URI=postgresql://chirp:chirp_dev_password@localhost:5432/chirp_cms tsx scripts/migrate-data-to-postgres-improved.ts",
    "migrate:direct-sql": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/migrate-direct-to-postgres.ts",
    "migrate:final": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts DATABASE_URI=postgresql://chirp:chirp_dev_password@localhost:5432/chirp_cms tsx scripts/migrate-final.ts",
    "migrate:all-sql": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts DATABASE_URI=postgresql://chirp:chirp_dev_password@localhost:5432/chirp_cms tsx scripts/migrate-all-direct-sql.ts",
    "migrate:remaining": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts DATABASE_URI=postgresql://chirp:chirp_dev_password@localhost:5432/chirp_cms tsx scripts/migrate-remaining-collections.ts",
    "migrate:media": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts DATABASE_URI=postgresql://chirp:chirp_dev_password@localhost:5432/chirp_cms tsx scripts/migrate-media.ts",
    "migrate:to-rds": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/migrate-to-aws-rds.ts",
    "migrate:to-rds-api": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/migrate-payload-api.ts",
    "migrate:fix-remaining": "cross-env PAYLOAD_CONFIG_PATH=payload.config.ts tsx scripts/fix-and-migrate-remaining.ts",
    "backup": "bash scripts/backup.sh",
    "prepare": "husky"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "^3.61.0",
    "@payloadcms/db-sqlite": "^3.61.0",
    "@payloadcms/next": "^3.61.0",
    "@payloadcms/plugin-redirects": "^3.61.0",
    "@payloadcms/richtext-lexical": "^3.61.0",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.44.7",
    "express": "^4.19.2",
    "graphql": "^16.11.0",
    "next": "^15.5.6",
    "nodemailer": "^7.0.10",
    "payload": "^3.61.0",
    "postgres": "^3.4.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "resend": "^6.4.0",
    "sharp": "^0.34.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/react": "^19.2.2",
    "better-sqlite3": "^12.4.6",
    "cross-env": "^7.0.3",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.4",
    "nodemon": "^3.1.0",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.3.3",
    "typescript-eslint": "^8.44.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
